<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.NameActivity">

    <TextView
        android:id="@+id/txtVwNme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="57dp"
        android:layout_marginTop="104dp"
        android:text="Name:" />

    <TextView
        android:id="@+id/nmeVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/txtVwNme"
        android:layout_alignParentStart="true"
        android:layout_marginStart="148dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/txtVwMng"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/txtVwNme"
        android:layout_alignParentTop="true"
        android:layout_marginTop="145dp"
        android:text="Meaning:" />

    <TextView
        android:id="@+id/mngVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/txtVwMng"
        android:layout_alignEnd="@+id/nmeVal"
        android:text="TextView" />
</RelativeLayout>